{
	"name": "vanilla/garden-schema",
	"description": "A simple data validation and cleaning library based on JSON Schema.",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Todd Burry",
			"email": "todd@vanillaforums.com"
		}
	],
	"config": {
		"platform": {
			"php": "8.1"
		}
	},
	"require": {
		"php": ">=8.1"
	},
	"autoload": {
		"psr-4": {
			"Garden\\Schema\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Garden\\Schema\\Tests\\": "tests"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "~9.0"
	}
}
